MODULES += os/services/slip-cmd
MODULES += os/services/shell

MAKE_MAC = MAKE_MAC_OTHER
MAKE_NET = MAKE_NET_IPV6


PREFIX ?= fd00::1/64
connect-router:	if-router.native
	sudo rlwrap ./if-router.native $(PREFIX)

connect-router-cooja:	if-router.native
	sudo rlwrap ./if-router.native -a localhost $(PREFIX)
